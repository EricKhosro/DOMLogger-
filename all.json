{
  "hooks": {
    "DOM": {
      "attribute": [
        "set:Element.prototype.innerHTML",
        "set:HTMLAnchorElement.prototype.href",
        "set:HTMLIFrameElement.prototype.src",
        "set:HTMLIFrameElement.prototype.srcdoc",
        "set:HTMLScriptElement.prototype.innerHTML",
        "set:HTMLScriptElement.prototype.src"
      ]
    },
    "EVAL": {
      "function": ["eval", "setInterval", "setTimeout"]
    },
    "NAVIGATION": {
      "function": [
        "history.pushState",
        "history.replaceState",
        "navigation.navigate",
        "window.open",
        "window.location"
      ],
      "attribute": [
        "set:Window.prototype.location",
        "set:Document.prototype.location"
      ]
    },
    "PSTMSG": {
      "function": ["window.postMessage"],
      "event": ["message"]
    },
    "URL params": {
      "function": [
        "URLSearchParams.prototype.get",
        "URLSearchParams.prototype.getAll",
        "URLSearchParams.prototype.has",
        "URLSearchParams",
        "ReadonlyURLSearchParams.prototype.get",
        "ReadonlyURLSearchParams.prototype.getAll",
        "ReadonlyURLSearchParams.prototype.has",
        "useSearchParams"
      ]
    },
    "storage": {
      "function": [
        "localStorage.getItem",
        "localStorage.setItem",
        "sessionStorage.getItem",
        "sessionStorage.setItem"
      ]
    }
  },
  "config": {}
}
